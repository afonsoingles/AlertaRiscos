{
  "expo": {
    "name": "AlertaRiscos",
    "slug": "AlertaRiscos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afonsoingles.alertariscos",
      "infoPlist": {
        "com.apple.developer.usernotifications.critical-alerts": 1
      },
      "entitlements": {
        "com.apple.developer.usernotifications.critical-alerts": true
      }
    },
    "android": {
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.afonsoingles.alertariscos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["onesignal-expo-plugin", {
        "mode": "development"
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "bdbefb2f-8edd-471e-ac5b-a2c7152a7171"
      }
    }
  }
}
